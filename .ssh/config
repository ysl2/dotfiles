Host *
    Compression yes
    ControlMaster auto
    ControlPersist yes
    HostKeyAlgorithms +ssh-rsa
    PubkeyAcceptedAlgorithms +ssh-rsa
    ServerAliveInterval 60
    ServerAliveCountMax 10

Host github.com
    HostName ssh.github.com
    User git
    Port 443

Host gitlab.com
    Hostname altssh.gitlab.com
    User git
    Port 443

Host *
    Include ~/.ssh/config.localhost
